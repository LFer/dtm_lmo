				<field name="aadfixed_minit"/>
				<field name="aadfixed_unit_amount"/>
				<field name="aadfixed_total_amount"/>
			</tree>
		</field>	
	</record>

                <!--Vista Form-->
	<record id="view_lmo_fix_form" model="ir.ui.view">
		<field name="name">lmo.fix.form</field>
		<field name="model">lmo.fix</field>
		<field name="arch" type="xml">
			<form string="Fixed credits and deductions" version="7.0">
				<sheet>
					<group>
						<field name="payroll_id"/>
						<field name="employee_id"/>
						<field name="code_id"/>
						<field name="section_id"/>
						<field name="aadfixed_unit"/>
						<field name="aadfixed_unit_amount"/>
						<field name="aadfixed_day"/>
						<field name="aadfixed_hour"/>
						<field name="aadfixed_minit"/>
						<field name="aadfixed_total_amount"/>
				</group>
			</sheet>
		</form>
	</field>	
	</record>
    
    
    
