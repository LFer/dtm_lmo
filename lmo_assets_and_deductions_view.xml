        <!--ESTA ES TODA LA VISTA PARA EL FIX-->
        
        
                  <!--Accion-->	
	<record id="action_lmo_fix_form" model="ir.actions.act_window">
		<field name="name">Fixed credits and deductions</field>
		<field name="res_model">lmo.fix</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>
	

                <!--Vista Tree-->
	<record id="view_lmo_fix_tree" model="ir.ui.view">
		<field name="name">lmo.fix.form</field>
		<field name="model">lmo.fix</field>
		<field name="arch" type="xml">
			<tree string="Fixed credits and deductions">
				<field name="payroll_id"/>
				<field name="employee_id"/>
				<field name="code_id"/>
				<field name="section_id"/>
				<field name="aadfixed_unit"/>
